@page
@model AbcBooks.Web.Areas.Identity.Pages.Account.Manage.WishlistModel
@{
	ViewData["Title"] = "Your Wishlist";
}

<div class="row">
	<div class="col-12 col-md-6">
		<h3>@ViewData["Title"]</h3>
	</div>
</div>
<hr />

<div class="table-scroll">
	<table id="wishlistTbl" class="table table-hover">
		<thead>
			<tr>
				<th>
					Title
				</th>
				<th>
					Description
				</th>
				<th></th>
			</tr>
		</thead>
	</table>
</div>

@section Scripts {
	<script src="~/js/wishlist.js"></script>
}